{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "baseUrl": "../../../../",
    "esModuleInterop": true,
    "allowJs": true,
    "types": ["vite/client", "vite-plugin-svgr/client", "vitest/globals", "lodash"],
    "paths": {
      "@iconify/react": [
        "node_modules/@headlamp-k8s/headlamp-plugin/node_modules/@iconify/react/src/icon.js"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/Crd": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/crd.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/K8s/*": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/k8s/*/index.d.ts",
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/k8s/*.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/*": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/*/index.d.ts",
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/*.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/ApiProxy": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/k8s/apiProxy.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/Utils": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/lib/util.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/CommonComponents": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/components/common/index.d.ts"
      ],
      "@headlamp-k8s/headlamp-plugin/lib/CommonComponents/*": [
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/components/common/*/index.d.ts",
        "node_modules/@headlamp-k8s/headlamp-plugin/lib/components/common/*.d.ts"
      ]
    },
    "rootDirs": ["node_modules/@headlamp-k8s/headlamp-plugin/"],
    "skipLibCheck": true
  },
  "include": ["./../../../../src/**/*"]
}
